(function(){
  function wrapToggle(fnName, panelId){
    if (typeof window[fnName] !== "function") return;
    const original = window[fnName];
    window[fnName] = function(open=true){
      const r = original.apply(this, arguments);
      try { var p=document.getElementById(panelId); var isOpen=open; if (p) isOpen = p.style.display!=="none"; document.body.classList.toggle("modal-open", !!isOpen); } catch(e){}
      return r;
    };
  }
  wrapToggle("openHelp","help");
  wrapToggle("openValidate","validate");
  document.addEventListener("DOMContentLoaded", function(){
    ["btn-help","btn-validate","btn-clear","btn-export","btn-desc"].forEach(function(id){
      var b=document.getElementById(id); if (!b) return;
      if (b.textContent && /<\s*$/.test(b.textContent)) b.textContent = b.textContent.replace(/<\s*$/,"").trim();
    });
    var desc=document.getElementById("btn-desc"); if (desc) desc.classList.add("primary");
  });
})();
</script><script defer="True" id="taskmgr-js-inline">
(function(){
  const $ = s=>document.querySelector(s);
  let editingId = null;
  const LS_KEY_DATA = "esf_user_data_v1";
  const LS_KEY_SNAP = "esf_snapshots_v1";

  function showMgr(){ $("#tmgr-overlay").style.display="flex"; document.body.classList.add("modal-open"); fillProcessSelect(); refreshList(); }
  function hideMgr(){ $("#tmgr-overlay").style.display="none"; document.body.classList.remove("modal-open"); clearForm(); }
  function bindOpeners(){
    // Insert button inline with CLEAR ALL (same container) 
    const clearBtn = document.getElementById("btn-clear");
    if (clearBtn && !document.getElementById("btn-taskmgr-inline")){
      const b = document.createElement("button");
      b.id = "btn-taskmgr-inline"; b.className = "btn primary"; b.textContent = "Task Manager";
      clearBtn.parentNode.insertBefore(b, clearBtn.nextSibling);
      b.addEventListener("click", showMgr);
    }
    // Keyboard shortcut
    document.addEventListener("keydown", (e)=>{
      if ((e.ctrlKey||e.metaKey) && e.key.toLowerCase()==="m"){ e.preventDefault(); showMgr(); }
    });
  }

  function ensureIds(){
    if (!window.TASKS) return;
    let maxId = (TASKS.reduce((mx,x)=>Math.max(mx, x && x.__id || 0), 0));
    TASKS.forEach(t=>{ if(!t.__id){ maxId += 1; t.__id = maxId; } });
  }
  function msg(text){
    const box = $("#tmgr-msg"); if (!box) return;
    const p = document.createElement("div");
    const now = new Date().toLocaleString();
    p.textContent = `[${now}] ${text}`; box.prepend(p);
  }
  function clearForm(){
    editingId = null;
    $("#tmgr-process").value = "";
    $("#tmgr-subproc").value = "";
    $("#tmgr-position").value = "";
    $("#tmgr-title-in").value = "";
    $("#tmgr-desc").value = "";
    $("#tmgr-freq").value = "ONGOING";
    $("#tmgr-imp").value = "HIGH";
    $("#tmgr-owner").value = "";
    $("#tmgr-exec").value = "";
    $("#tmgr-update").disabled = true;
    $("#tmgr-delete").disabled = true;
  }
  function fillProcessSelect(){
    const sel = $("#tmgr-process"); if (!sel) return;
    const opts = (window.PROCESSES||[]).map(p=>`<option value="${p.id}">${p.name||p.id}</option>`).join("");
    sel.innerHTML = `<option value="">(new process)</option>` + opts;
  }
  function computeTotals(list){
    const r={tasks:0,high:0,med:0,low:0};
    (list||[]).forEach(t=>{
      r.tasks++;
      const imp = String(t.importance||"").toUpperCase();
      if (imp==="HIGH") r.high++; else if (imp==="MEDIUM") r.med++; else if (imp==="LOW") r.low++;
    });
    return r;
  }
  function rebuildUI(){
    try{ if (typeof buildTree==="function") buildTree(); }catch(e){}
    try{ if (typeof fillSelects==="function") fillSelects(); }catch(e){}
    try{ if (typeof applyFilters==="function") applyFilters(); }catch(e){}
    try{
      const totals = computeTotals(window.TASKS||[]);
      if (typeof buildKPIs==="function") buildKPIs(totals);
    }catch(e){}
  }
  function escapeHtml(s){ return String(s).replace(/[&<>\"']/g, m=>({ "&":"&amp;","<":"&lt;",">":"&gt;","\"":"&quot;","'":"&#39;" }[m])); }
  function renderList(filter=""){
    const tb = document.getElementById("tmgr-tbody"); if (!tb) return;
    ensureIds();
    const q = filter.trim().toLowerCase();
    const rows = (window.TASKS||[]).filter(t=>{
      if (!q) return true;
      const hay = [
        t.title||"", t.owner||"", t.exec||"", t.subproc||"",
        (t.group||"").toString().toLowerCase(), t.process||""
      ].join(" ").toLowerCase();
      return hay.includes(q);
    }).slice(0,800).map(t=>`<tr>
      <td>${escapeHtml(t.title||"")}</td>
      <td>${escapeHtml((t.group||"").toUpperCase()||t.process||"")}</td>
      <td>${escapeHtml(t.subproc||"")}</td>
      <td>${escapeHtml((t.importance||"").toUpperCase())}</td>
      <td>${escapeHtml(t.owner||"")}</td>
      <td>${escapeHtml(t.exec||"")}</td>
      <td><button class="btn" data-pick="${t.__id}">Pick</button></td>
    </tr>`).join("");
    tb.innerHTML = rows || "<tr><td colspan='7'>Nu există task-uri încă.</td></tr>";
  }
  function refreshList(){ renderList($("#tmgr-search")?.value||""); }

  // Autosave + snapshots
  function snapshot(){
    try{
      const data = { processes: PROCESSES, tasks: TASKS, validation: (window.VALID||{}) };
      const snap = { ts: Date.now(), data };
      const list = JSON.parse(localStorage.getItem(LS_KEY_SNAP)||"[]");
      list.unshift(snap);
      while(list.length>5) list.pop();
      localStorage.setItem(LS_KEY_SNAP, JSON.stringify(list));
    }catch(e){}
  }
  function autosave(){
    try{
      const data = { processes: PROCESSES, tasks: TASKS, validation: (window.VALID||{}) };
      localStorage.setItem(LS_KEY_DATA, JSON.stringify(data));
    }catch(e){}
  }
  function restoreAutosave(){
    try{
      const raw = localStorage.getItem(LS_KEY_DATA);
      if (!raw) { msg("Nu există autosave."); return; }
      const payload = JSON.parse(raw);
      if (Array.isArray(payload.tasks)){
        TASKS.splice(0,TASKS.length, ...payload.tasks);
        if (Array.isArray(PAYLOAD?.tasks)) PAYLOAD.tasks.splice(0,PAYLOAD.tasks.length, ...payload.tasks);
      }
      if (Array.isArray(payload.processes)){
        PROCESSES.splice(0,PROCESSES.length, ...payload.processes);
      }
      msg("♻️ Restaurat din autosave."); rebuildUI(); refreshList();
    }catch(e){ msg("Eroare la restore: "+e.message); }
  }

  // Read/write
  function readForm(){
    const proc = document.getElementById("tmgr-process").value.trim();
    const sub  = document.getElementById("tmgr-subproc").value.trim();
    const pos  = document.getElementById("tmgr-position").value.trim();
    const tit  = document.getElementById("tmgr-title-in").value.trim();
    const desc = document.getElementById("tmgr-desc").value.trim();
    const freq = document.getElementById("tmgr-freq").value.trim().toUpperCase();
    const imp  = document.getElementById("tmgr-imp").value.trim().toUpperCase();
    const owner= document.getElementById("tmgr-owner").value.trim();
    const exec = document.getElementById("tmgr-exec").value.trim();
    const processId = proc || (tit ? tit.toLowerCase().replace(/\s+/g,'-').slice(0,24) : "process-x");
    if (!tit){ msg("❗ Title este obligatoriu."); return null; }
    if (!sub){ msg("❗ Sub-process este obligatoriu."); return null; }
    return {
      process: processId,
      group: (proc? (PROCESSES.find(p=>p.id===proc)?.name || proc.replace(/-/g,' ').toUpperCase()) : processId.replace(/-/g,' ').toUpperCase()),
      subproc: sub,
      position: pos,
      title: tit,
      desc: desc,
      freq: freq || "ONGOING",
      importance: imp || "HIGH",
      owner: owner,
      exec: exec
    };
  }
  function ensureProcessExists(task){
    if (!task || !task.process) return;
    const id = task.process;
    const found = (PROCESSES||[]).find(p=>p.id===id);
    if (!found){
      PROCESSES.push({id, name:id.replace(/-/g,' ').toUpperCase(), subs: task.subproc? [task.subproc] : []});
    } else if (task.subproc){
      if (!Array.isArray(found.subs)) found.subs = [];
      if (!found.subs.includes(task.subproc)) found.subs.push(task.subproc);
    }
  }

  // Events
  document.addEventListener("click", (e)=>{
    const pickBtn = e.target.closest("button[data-pick]");
    if (pickBtn) {
      const id = Number(pickBtn.getAttribute("data-pick"));
      const t = (window.TASKS||[]).find(x=>x.__id===id);
      if (!t) return;
      editingId = id;
      fillProcessSelect();
      document.getElementById("tmgr-process").value = t.process || "";
      document.getElementById("tmgr-subproc").value = t.subproc || "";
      document.getElementById("tmgr-position").value = t.position || "";
      document.getElementById("tmgr-title-in").value = t.title || "";
      document.getElementById("tmgr-desc").value = t.desc || "";
      document.getElementById("tmgr-freq").value = (String(t.freq||"").toUpperCase()||"ONGOING");
      document.getElementById("tmgr-imp").value = (String(t.importance||"").toUpperCase()||"HIGH");
      document.getElementById("tmgr-owner").value = t.owner || "";
      document.getElementById("tmgr-exec").value = t.exec || "";
      document.getElementById("tmgr-update").disabled = false;
      document.getElementById("tmgr-delete").disabled = false;
      return;
    }
  });
  document.addEventListener("click", (e)=>{
    const addBtn = e.target.closest("#tmgr-add");
    const updBtn = e.target.closest("#tmgr-update");
    const delBtn = e.target.closest("#tmgr-delete");
    const impBtn = e.target.closest("#tmgr-import");
    const expBtn = e.target.closest("#tmgr-export");
    const clsBtn = e.target.closest("#tmgr-close");
    const xBtn   = e.target.closest("#tmgr-x");
    const clrBtn = e.target.closest("#tmgr-clear");
    const rstBtn = e.target.closest("#tmgr-restore");
    if (addBtn){
      const t = readForm(); if (!t) return;
      ensureIds();
      t.__id = (TASKS.reduce((mx,x)=>Math.max(mx, x.__id||0), 0) + 1);
      TASKS.push(t); if (Array.isArray(PAYLOAD?.tasks)) PAYLOAD.tasks.push(t);
      ensureProcessExists(t);
      msg("✅ Task adăugat."); snapshot(); autosave(); refreshList(); rebuildUI(); return;
    }
    if (updBtn){
      if (!editingId) return;
      const upd = readForm(); if (!upd) return;
      const idx = TASKS.findIndex(x=>x.__id===editingId);
      if (idx>-1){
        const keepId = TASKS[idx].__id;
        TASKS[idx] = Object.assign({}, TASKS[idx], upd, {__id: keepId});
      }
      if (Array.isArray(PAYLOAD?.tasks)){
        const j = PAYLOAD.tasks.findIndex(x=>x.__id===editingId);
        if (j>-1){ PAYLOAD.tasks[j] = TASKS[idx]; }
      }
      ensureProcessExists(upd);
      msg("✅ Modificare salvată."); snapshot(); autosave(); refreshList(); rebuildUI(); return;
    }
    if (delBtn){
      if (!editingId) return;
      if (!confirm("Ștergi acest task?")) return;
      const idx = TASKS.findIndex(x=>x.__id===editingId);
      if (idx>-1){ TASKS.splice(idx,1); }
      if (Array.isArray(PAYLOAD?.tasks)){
        const j = PAYLOAD.tasks.findIndex(x=>x.__id===editingId);
        if (j>-1){ PAYLOAD.tasks.splice(j,1); }
      }
      msg("🗑️ Task șters."); snapshot(); autosave(); clearForm(); refreshList(); rebuildUI(); return;
    }
    if (impBtn){ document.getElementById("tmgr-file").click(); return; }
    if (expBtn){
      const data = { processes: PROCESSES, tasks: TASKS, validation: (window.VALID||{}) };
      const blob = new Blob([JSON.stringify(data,null,2)], {type:"application/json"});
      const url = URL.createObjectURL(blob);
      const a = document.createElement("a"); a.href = url; a.download = "ESF_Task_Tree_updated.json";
      document.body.appendChild(a); a.click(); setTimeout(()=>{ URL.revokeObjectURL(url); a.remove(); }, 800);
      return;
    }
    if (clsBtn || xBtn){ hideMgr(); return; }
    if (clrBtn){ clearForm(); msg("Form cleared."); return; }
    if (rstBtn){ restoreAutosave(); return; }
  });
  document.addEventListener("input", (e)=>{ if (e.target && e.target.id==="tmgr-search"){ renderList(e.target.value||""); } });
  document.addEventListener("change", async (e)=>{
    if (e.target && e.target.id==="tmgr-file"){
      const file = e.target.files && e.target.files[0]; if (!file) return;
      try{
        const text = await file.text();
        const lower = file.name.toLowerCase();
        let incoming;
        if (lower.endsWith(".json")) incoming = JSON.parse(text);
        else if (lower.endsWith(".csv")) incoming = csvToPayload(text);
        else throw new Error("Folosește .json sau .csv (Excel → Save as CSV).");
        normalizeIncoming(incoming);
        if (document.getElementById("tmgr-replace").checked){
          TASKS.splice(0, TASKS.length, ...(incoming.tasks||[]));
          if (Array.isArray(PAYLOAD?.tasks)) PAYLOAD.tasks.splice(0, PAYLOAD.tasks.length, ...(incoming.tasks||[]));
          PROCESSES.splice(0, PROCESSES.length, ...(incoming.processes||[]));
        } else {
          (incoming.tasks||[]).forEach(t=>{ t.__id = (TASKS.reduce((mx,x)=>Math.max(mx, x.__id||0), 0) + 1); TASKS.push(t); if (Array.isArray(PAYLOAD?.tasks)) PAYLOAD.tasks.push(t); ensureProcessExists(t); });
        }
        msg("✅ Import reușit."); snapshot(); autosave(); refreshList(); rebuildUI();
      }catch(err){ msg("Eroare la import: " + (err && err.message ? err.message : err)); }
      finally{ e.target.value=""; }
    }
  });

  function csvToPayload(text){
    const rows = text.split(/\r?\n/).filter(Boolean);
    if (rows.length<2) throw new Error("CSV must have header + rows.");
    const header = rows[0].split(",").map(s=>s.trim().toUpperCase());
    const tasks = rows.slice(1).map(line=>{
      const vals=line.split(",").map(s=>s.trim()); const o={};
      header.forEach((h,i)=>{
        const v=(vals[i]||"").trim();
        if (h==="PROCESS") o.process=v.toLowerCase().replace(/\s+/g,"-");
        if (h==="SUB-PROCESS") o.subproc=v;
        if (h==="POSITION") o.position=v;
        if (h==="KEY ACTIVITY" || h==="TASK" || h==="TITLE") o.title=v;
        if (h==="DESCRIPTION" || h==="DESC") o.desc=v;
        if (h==="FREQUENCY" || h==="FREQ") o.freq=v.toUpperCase();
        if (h==="IMPORTANCE" || h==="IMP") o.importance=v.toUpperCase();
        if (h==="OWNER") o.owner=v;
        if (h==="EXECUTOR" || h==="EXEC") o.exec=v;
      });
      o.group=(o.process||"").replace(/-/g," ").toUpperCase();
      return o;
    });
    const procMap={};
    tasks.forEach(t=>{ if(!t.process) return; procMap[t.process]=procMap[t.process]||new Set(); if (t.subproc) procMap[t.process].add(t.subproc); });
    const processes = Object.entries(procMap).map(([id,subs])=>({id, name:id.replace(/-/g," ").toUpperCase(), subs:[...subs]}));
    return { processes, tasks };
  }
  function normalizeIncoming(incoming){
    (incoming.tasks||[]).forEach(t=>{
      t.freq = String(t.freq||"").toUpperCase();
      t.importance = String(t.importance||"").toUpperCase();
      t.process = String(t.process||"").toLowerCase();
      t.group = t.group || t.process.replace(/-/g," ").toUpperCase();
    });
    if (!incoming.processes || !incoming.processes.length){
      const procMap={};
      (incoming.tasks||[]).forEach(t=>{ const pid=t.process; if(!pid) return; procMap[pid]=procMap[pid]||new Set(); if (t.subproc) procMap[pid].add(t.subproc); });
      incoming.processes = Object.entries(procMap).map(([id,subs])=>({id, name:id.replace(/-/g," ").toUpperCase(), subs:[...subs]}));
    }
  }

  // Mount inline button after DOM is ready
  document.addEventListener("DOMContentLoaded", bindOpeners);
})();
