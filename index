// v8 SAFE RELOAD + WAIT-FOR-INIT
(function(){
  const KEY = "esf_replace_payload";
  function applyIncoming(incoming){
    // Merge in-place into PAYLOAD/TASKS/PROCESSES/VALID
    if (window.PAYLOAD && typeof window.PAYLOAD === "object") {
      if (!Array.isArray(window.PAYLOAD.processes)) window.PAYLOAD.processes = [];
      window.PAYLOAD.processes.splice(0, window.PAYLOAD.processes.length, ...(incoming.processes || []));
      if (!Array.isArray(window.PAYLOAD.tasks)) window.PAYLOAD.tasks = [];
      window.PAYLOAD.tasks.splice(0, window.PAYLOAD.tasks.length, ...(incoming.tasks || []));
      if (!window.PAYLOAD.validation) window.PAYLOAD.validation = {};
      var totals = { tasks: (incoming.tasks||[]).length, high:0, med:0, low:0 };
      (incoming.tasks||[]).forEach(t=>{
        const imp = String(t.importance||"").toUpperCase();
        if (imp==="HIGH") totals.high++; else if (imp==="MEDIUM") totals.med++; else if (imp==="LOW") totals.low++;
      });
      const valid = incoming.validation || { totals };
      Object.keys(window.PAYLOAD.validation).forEach(k=>delete window.PAYLOAD.validation[k]);
      Object.assign(window.PAYLOAD.validation, valid);
    }
    if (typeof window.PROCESSES !== "undefined") window.PROCESSES = window.PAYLOAD.processes;
    if (typeof window.TASKS !== "undefined" && Array.isArray(window.TASKS)) window.TASKS.splice(0, window.TASKS.length, ...window.PAYLOAD.tasks);
    if (typeof window.VALID !== "undefined" && window.VALID) { Object.keys(window.VALID).forEach(k=>delete window.VALID[k]); Object.assign(window.VALID, window.PAYLOAD.validation); }
  }
  function rebuild(){
    if (typeof window.buildTree === "function") window.buildTree();
    if (typeof window.fillSelects === "function") window.fillSelects();
    if (typeof window.applyFilters === "function") window.applyFilters();
    if (typeof window.buildKPIs === "function") window.buildKPIs(window.PAYLOAD.validation.totals);
  }
  function toast(){
    const note = document.createElement("div");
    note.textContent = "✅ Upload aplicat după reload.";
    note.style.position="fixed"; note.style.right="16px"; note.style.bottom="16px";
    note.style.background="#e8f8ef"; note.style.border="1px solid #20a464";
    note.style.padding="10px 14px"; note.style.borderRadius="10px"; note.style.zIndex="9999";
    document.body.appendChild(note);
    setTimeout(()=>note.remove(), 3000);
  }

  const raw = sessionStorage.getItem(KEY);
  if (!raw) return;
  const incoming = JSON.parse(raw);
  sessionStorage.removeItem(KEY);

  // Wait for the app to be ready (PAYLOAD + buildTree etc.)
  let tries = 0;
  const timer = setInterval(()=>{
    tries++;
    const ready = !!(window.PAYLOAD && (typeof window.buildTree === "function" || document.getElementById("tree")));
    if (ready || tries > 100) {
      clearInterval(timer);
      try { applyIncoming(incoming); rebuild(); toast(); } catch(e){ console.error("v8 bootstrap error:", e); }
    }
  }, 100);
})();
</script><script defer="True">
// v8 Data Check Upload UI (stores payload then triggers reload)
(function(){
  function byId(id){ return document.getElementById(id); }
  function msg(txt, type){
    const val = byId("valContent"); if (!val) return;
    const d = document.createElement("div"); d.innerHTML = txt;
    d.style.margin="6px 0"; d.style.padding="6px 8px"; d.style.borderRadius="10px";
    d.style.background = type==="ok" ? "#e8f8ef" : (type==="warn" ? "#fff4e5" : "#ffe8e8");
    d.style.border = type==="ok" ? "1px solid #20a464" : (type==="warn" ? "1px solid #f28c2a" : "1px solid #d33");
    val.prepend(d);
  }
  function styleBtn(b){ b.style.padding="8px 14px"; b.style.border="1px solid #f28c2a"; b.style.borderRadius="12px"; b.style.cursor="pointer"; b.style.background="white"; }

  async function parseFile(file){
    const text = await file.text(); const lower = file.name.toLowerCase();
    let incoming;
    if (lower.endsWith(".json")) incoming = JSON.parse(text);
    else if (lower.endsWith(".csv")){
      const rows = text.split(/\r?\n/).filter(Boolean); if (rows.length<2) throw new Error("CSV must have header + rows.");
      const header = rows[0].split(",").map(s=>s.trim().toUpperCase());
      const tasks = rows.slice(1).map(line=>{
        const vals=line.split(",").map(s=>s.trim()); const o={};
        header.forEach((h,i)=>{
          const v=(vals[i]||"").trim();
          if (h==="PROCESS") o.process = v.toLowerCase().replace(/\s+/g,"-");
          if (h==="SUB-PROCESS") o.subproc = v;
          if (h==="POSITION") o.position = v;
          if (h==="KEY ACTIVITY") o.title = v;
          if (h==="DESCRIPTION") o.desc = v;
          if (h==="FREQUENCY") o.freq = v.toUpperCase();
          if (h==="IMPORTANCE") o.importance = v.toUpperCase();
          if (h==="OWNER") o.owner = v;
          if (h==="EXECUTOR") o.exec = v;
        });
        o.group = (o.process||"").replace(/-/g," ").toUpperCase();
        return o;
      });
      const procMap={}; tasks.forEach(t=>{ if(!t.process) return; procMap[t.process]=procMap[t.process]||new Set(); if(t.subproc) procMap[t.process].add(t.subproc); });
      const processes = Object.entries(procMap).map(([id,subs])=>({id, name:id.replace(/-/g," ").toUpperCase(), subs:[...subs]}));
      incoming = { processes, tasks };
    } else throw new Error("Folosește .json sau .csv");
    (incoming.tasks||[]).forEach(t=>{ t.freq=String(t.freq||"").toUpperCase(); t.importance=String(t.importance||"").toUpperCase(); t.process=String(t.process||"").toLowerCase(); t.group=t.group||t.process.replace(/-/g," ").toUpperCase(); });
    return incoming;
  }

  function validateData(incoming){
    const processes=(incoming.processes||[]).map(p=>p.id);
    const subsByProc=new Map((incoming.processes||[]).map(p=>[p.id,new Set(p.subs||[])]));
    const unknown_process=[], unknown_subproc=[], missing_critical=[]; let high=0,med=0,low=0;
    (incoming.tasks||[]).forEach((t,i)=>{
      const row=i+1;
      ["process","subproc","position","title","freq","importance"].forEach(k=>{ if(!t[k]||String(t[k]).trim()==="") missing_critical.push(row); });
      if (t.importance==="HIGH") high++; else if (t.importance==="MEDIUM") med++; else if (t.importance==="LOW") low++;
      if (!processes.includes(t.process)) unknown_process.push({row, value:(t.group||t.process||"").toString().toUpperCase()});
      const set=subsByProc.get(t.process);
      if (set && t.subproc && !set.has(t.subproc)) unknown_subproc.push({row, process:(t.group||"").toString().toUpperCase(), value:t.subproc});
    });
    return { totals:{tasks:(incoming.tasks||[]).length, high, med, low}, unknown_process, unknown_subproc, missing_critical };
  }

  function renderBar(valEl){
    if (!valEl || document.getElementById("dc-import-bar")) return;
    const bar=document.createElement("div");
    bar.id="dc-import-bar";
    bar.style.display="grid"; bar.style.gridTemplateColumns="auto auto auto"; bar.style.gap="10px";
    bar.style.alignItems="center"; bar.style.border="1px dashed #f28c2a"; bar.style.padding="10px 12px"; bar.style.borderRadius="12px"; bar.style.margin="8px 0 12px 0"; bar.style.background="#fffaf6";

    const btnChoose=document.createElement("button"); btnChoose.textContent="Choose file"; styleBtn(btnChoose);
    const btnValidate=document.createElement("button"); btnValidate.textContent="Validate file"; styleBtn(btnValidate); btnValidate.disabled=true;
    const btnApply=document.createElement("button"); btnApply.textContent="Apply to interface"; styleBtn(btnApply); btnApply.disabled=true;
    const input=document.createElement("input"); input.type="file"; input.accept=".json,.csv"; input.style.display="none";

    function showPreview(v){
      const val = valEl;
      const lines=[];
      lines.push(`Tasks: ${v.totals.tasks} (HIGH ${v.totals.high}, MEDIUM ${v.totals.med}, LOW ${v.totals.low})`);
      if (v.missing_critical.length) lines.push(`Missing critical fields (row): ${v.missing_critical.slice(0,50).join(", ")}`);
      if (v.unknown_process.length){ lines.push(`Unknown PROCESS (${v.unknown_process.length}):`); v.unknown_process.slice(0,50).forEach(x=>lines.push(`  - row ${x.row}: "${x.value}"`)); }
      if (v.unknown_subproc.length){ lines.push(`Unknown SUB-PROCESS (${v.unknown_subproc.length}):`); v.unknown_subproc.slice(0,50).forEach(x=>lines.push(`  - row ${x.row} [${x.process}]: "${x.value}"`)); }
      val.innerHTML = "<pre style='white-space:pre-wrap;'>" + lines.join("\n") + "</pre>";
    }

    btnChoose.addEventListener("click", ()=>input.click());
    input.addEventListener("change", async ()=>{
      const f=input.files&&input.files[0]; if (!f) return;
      try{ const incoming=await parseFile(f); window._uploadedData=incoming; btnValidate.disabled=false; btnApply.disabled=true; msg("Fișier încărcat. Apasă 'Validate file'.","warn"); }
      catch(e){ msg("Eroare la încărcare: "+(e&&e.message?e.message:e),"err"); }
      finally{ input.value=""; }
    });
    btnValidate.addEventListener("click", ()=>{
      const data=window._uploadedData; if (!data){ msg("Încarcă un fișier mai întâi.","warn"); return; }
      const v=validateData(data); window._uploadedValid=v; showPreview(v);
      if (v.missing_critical && v.missing_critical.length){ btnApply.disabled=true; msg("Sunt câmpuri critice lipsă. Corectează fișierul și validează din nou.","err"); }
      else { btnApply.disabled=false; msg("Validare OK. Poți apăsa 'Apply to interface'.","ok"); }
    });
    btnApply.addEventListener("click", ()=>{
      try {
        const payload = window._uploadedData || {};
        sessionStorage.setItem("esf_replace_payload", JSON.stringify(payload));
        location.reload();
      } catch(e){ msg("Eroare la Apply: "+(e&&e.message?e.message:e),"err"); }
    });

    bar.appendChild(btnChoose); bar.appendChild(btnValidate); bar.appendChild(btnApply);
    valEl.parentNode.insertBefore(bar, valEl);
  }

  const obs=new MutationObserver(()=>{ const val=document.getElementById("valContent"); if (val) renderBar(val); });
  obs.observe(document.documentElement||document.body,{childList:true,subtree:true});
  if (typeof window.openValidate==="function"){ const _orig=window.openValidate; window.openValidate=function(show){ const r=_orig.apply(this,arguments); try{ const val=document.getElementById("valContent"); if (show&&val) renderBar(val);}catch(e){} return r; }; }
})();
</script><script defer="True" id="baked-payload-v10">
(function(){
  const INCOMING = {{
  "processes": [
    {{
      "id": "field-management",
      "name": "FIELD MANAGEMENT",
      "subs": [
        "Test"
      ]
    }}
  ],
  "tasks": [
    {{
      "process": "field-management",
      "subproc": "Test",
      "group": "FIELD MANAGEMENT",
      "position": "Test Position",
      "title": "Test Titlu",
      "desc": "Test descriere",
      "freq": "ONGOING",
      "importance": "HIGH",
      "owner": "Test",
      "exec": "Test 2"
    }}
  ],
  "validation": {{}}
}};
  function ensureProcesses(payload){
    if (!payload.processes || !payload.processes.length){
      const procMap={};
      (payload.tasks||[]).forEach(t=>{ const pid=String(t.process||"").toLowerCase(); if(!pid) return; procMap[pid]=procMap[pid]||new Set(); if (t.subproc) procMap[pid].add(t.subproc); });
      payload.processes = Object.entries(procMap).map(([id,subs])=>({id, name:id.replace(/-/g," ").toUpperCase(), subs:[...subs]}));
    }
  }
  function applyIncoming(incoming){
    ensureProcesses(incoming);
    if (!window.PAYLOAD || typeof window.PAYLOAD!=="object") return "no-payload";
    if (!Array.isArray(window.PAYLOAD.processes)) window.PAYLOAD.processes = [];
    window.PAYLOAD.processes.splice(0, window.PAYLOAD.processes.length, ...(incoming.processes||[]));
    if (!Array.isArray(window.PAYLOAD.tasks)) window.PAYLOAD.tasks = [];
    window.PAYLOAD.tasks.splice(0, window.PAYLOAD.tasks.length, ...(incoming.tasks||[]));
    if (!window.PAYLOAD.validation) window.PAYLOAD.validation = {};
    var totals={tasks:(incoming.tasks||[]).length, high:0, med:0, low:0};
    (incoming.tasks||[]).forEach(t=>{ const imp=String(t.importance||"").toUpperCase(); if(imp==="HIGH") totals.high++; else if(imp==="MEDIUM") totals.med++; else if(imp==="LOW") totals.low++; });
    const valid=incoming.validation||{ totals };
    Object.keys(window.PAYLOAD.validation).forEach(k=>delete window.PAYLOAD.validation[k]);
    Object.assign(window.PAYLOAD.validation, valid);
    if (typeof window.PROCESSES!=="undefined") window.PROCESSES = window.PAYLOAD.processes;
    if (typeof window.TASKS!=="undefined" && Array.isArray(window.TASKS)) window.TASKS.splice(0, window.TASKS.length, ...window.PAYLOAD.tasks);
    if (typeof window.VALID!=="undefined" && window.VALID){ Object.keys(window.VALID).forEach(k=>delete window.VALID[k]); Object.assign(window.VALID, window.PAYLOAD.validation); }
    if (typeof window.buildTree==="function") window.buildTree();
    if (typeof window.fillSelects==="function") window.fillSelects();
    if (typeof window.applyFilters==="function") window.applyFilters();
    if (typeof window.buildKPIs==="function") window.buildKPIs(window.PAYLOAD.validation.totals);
    setTimeout(()=>{ try{ const note=document.createElement("div"); note.textContent="✅ Bază de date injectată (baked-in)."; note.style.position="fixed"; note.style.right="16px"; note.style.bottom="16px"; note.style.background="#e8f8ef"; note.style.border="1px solid #20a464"; note.style.padding="10px 14px"; note.style.borderRadius="10px"; note.style.zIndex="9999"; document.body.appendChild(note); setTimeout(()=>note.remove(),3000); }catch(e){} }, 100);
  }
  // Wait for app init then inject
  let tries=0;
  const timer=setInterval(()=>{ 
    tries++;
    const ready = !!(window.PAYLOAD && (typeof window.buildTree==="function" || document.getElementById("tree")));
    if (ready || tries>100){ clearInterval(timer); try{ applyIncoming(INCOMING); }catch(e){ console.error("baked apply error:", e); } }
  }, 100);
})();
