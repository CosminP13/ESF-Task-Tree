  window.__rebuildUI = function(){
    try{ if (typeof buildTree==="function") buildTree(); }catch(e){}
    try{ if (typeof fillSelects==="function") fillSelects(); }catch(e){}
    try{ if (typeof applyFilters==="function") applyFilters(); }catch(e){}
    try{
      if (typeof buildKPIs==="function"){
        var list = (window.TASKS||[]);
        var totals={tasks:0,high:0,med:0,low:0};
        list.forEach(function(t){
          totals.tasks++;
          var imp = String(t.importance||"").toUpperCase();
          if (imp==="HIGH") totals.high++; else if (imp==="MEDIUM") totals.med++; else if (imp==="LOW") totals.low++;
        });
        buildKPIs(totals);
      }
    }catch(e){}
  };
})();
