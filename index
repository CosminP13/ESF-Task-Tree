</script><script defer="True" id="safe-overrides-step15">
(function(){
  function getFocusable(container){
    if(!container) return [];
    return Array.prototype.slice.call(
      container.querySelectorAll('a[href], button, input, select, textarea, [tabindex]:not([tabindex="-1"])')
    ).filter(function(el){ return !el.hasAttribute('disabled') && el.offsetParent !== null; });
  }
  function trap(modalEl){
    var panel = modalEl.querySelector('.panel') || modalEl;
    var previouslyFocused = null;
    function onOpen(){
      previouslyFocused = document.activeElement;
      var focusables = getFocusable(panel);
      if(focusables.length){
        focusables[0].focus();
      }else{
        panel.setAttribute('tabindex','-1');
        panel.focus();
      }
      document.addEventListener('keydown', onKeyDown, true);
    }
    function onClose(){
      document.removeEventListener('keydown', onKeyDown, true);
      if(previouslyFocused && document.body.contains(previouslyFocused)){
        previouslyFocused.focus();
      }
    }
    function onKeyDown(e){
      if(e.key !== 'Tab') return;
      var focusables = getFocusable(panel);
      if(!focusables.length) return;
      var first = focusables[0];
      var last = focusables[focusables.length - 1];
      if(e.shiftKey && document.activeElement === first){
        e.preventDefault(); last.focus();
      } else if(!e.shiftKey && document.activeElement === last){
        e.preventDefault(); first.focus();
      }
    }
    // Observe display changes to auto-enable trap
    var obs = new MutationObserver(function(){
      var visible = window.getComputedStyle(modalEl).display !== 'none';
      if(visible){ onOpen(); } else { onClose(); }
    });
    obs.observe(modalEl, { attributes:true, attributeFilter:['style','class'] });
    // In case it's initially visible:
    if(window.getComputedStyle(modalEl).display !== 'none'){ onOpen(); }
  }

  function init(){
    var skillsModal = document.getElementById('skills-modal');
    var posModal = document.getElementById('skills-positions');
    if(skillsModal) trap(skillsModal);
    if(posModal) trap(posModal);
  }
  if(document.readyState === 'loading'){
    document.addEventListener('DOMContentLoaded', init);
  } else { init(); }
})();
</script><script defer="True" id="cleanup-tasks-pill">
document.addEventListener('DOMContentLoaded', function(){
  var reExactTasks = /^\s*\d+\s*TASKS?\s*$/i;

  function shouldRemove(el){
    var txt = (el.textContent || "").trim();
    if(!reExactTasks.test(txt)) return false;
    // exclude any table/list areas (we only want the header pill under ESF)
    if(el.closest('table, .table, .tasks-table, .list, .grid, .datatable')) return false;
    // exclude section counters that include extra words (e.g., "TASKS SHOWN")
    if(/SHOWN/i.test(txt)) return false;
    return true;
  }

  // scan a few element types
  var nodes = document.querySelectorAll('button, a, span, div');
  nodes.forEach(function(n){
    if(shouldRemove(n)) n.remove();
  });

  // Also observe later dynamic injections
  var obs = new MutationObserver(function(muts){
    muts.forEach(function(m){
      (m.addedNodes || []).forEach(function(node){
        if(!(node instanceof HTMLElement)) return;
        if(shouldRemove(node)) node.remove();
        node.querySelectorAll && node.querySelectorAll('button, a, span, div').forEach(function(n){
          if(shouldRemove(n)) n.remove();
        });
      });
    });
  });
  obs.observe(document.body, {childList:true, subtree:true});
});
</script><script defer="True" id="desc-normalize-next">
(function(){
  function findDescriptionColumnIndexes(table){
    var headers = table.querySelectorAll('thead th');
    var idxs = [];
    headers.forEach(function(th, i){
      var t = (th.textContent || "").trim().toLowerCase();
      if(!t) return;
      if(t === "description" || t.indexOf("description") >= 0 || t === "descriere" || t.indexOf("descriere") >= 0){
        idxs.push(i);
      }
    });
    return idxs;
  }
  function addDescClass(table){
    var idxs = findDescriptionColumnIndexes(table);
    if(!idxs.length) return;
    var rows = table.querySelectorAll('tbody tr');
    rows.forEach(function(tr){
      idxs.forEach(function(i){
        var td = tr.children[i];
        if(td) td.classList.add('desc');
      });
    });
  }
  function isPlainTextWithBreaks(el){
    for(var i=0;i<el.childNodes.length;i++){
      var n = el.childNodes[i];
      if(n.nodeType === 3) continue;
      if(n.nodeType === 1 && n.tagName === 'BR') continue;
      return false;
    }
    return true;
  }
  function normalizeCell(td){
    if(td.getAttribute('data-cleaned') === '1') return;
    var html = td.innerHTML;
    if(!html || html.trim() === ""){ td.setAttribute('data-cleaned','1'); return; }
    if(td.querySelector('ul, ol, p, table, pre, code, blockquote')){ td.setAttribute('data-cleaned','1'); return; }
    if(!isPlainTextWithBreaks(td)){ td.setAttribute('data-cleaned','1'); return; }

    var tmp = td.innerHTML.replace(/<br\s*\/?>/gi, "\n");
    var lines = tmp.split(/\n+/).map(function(s){ return s.replace(/\u00A0/g, ' ').trim(); }).filter(Boolean);
    if(!lines.length){ td.setAttribute('data-cleaned','1'); return; }

    var bulletRE = /^\s*[-*•]\s+/;
    var hasBullets = lines.some(function(l){ return bulletRE.test(l); });
    var out = document.createElement(hasBullets ? 'ul' : 'div');
    if(hasBullets){
      lines.forEach(function(l){
        var text = l.replace(bulletRE, '');
        var li = document.createElement('li');
        li.textContent = text;
        out.appendChild(li);
      });
    } else {
      lines.forEach(function(l){
        var p = document.createElement('p');
        p.textContent = l;
        out.appendChild(p);
      });
    }
    td.innerHTML = "";
    td.appendChild(out);
    td.setAttribute('data-cleaned','1');
  }
  function linkifyTextNode(node){
    if(node.nodeType !== 3) return;
    var t = node.nodeValue;
    if(!t || !t.trim()) return;
    if(node.parentNode && node.parentNode.closest('a')) return;

    var frag = document.createDocumentFragment();
    var reURL = /\bhttps?:\/\/[^\s<>"']+/gi;
    var reEmail = /\b[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\.[A-Za-z]{2,}\b/g;
    var s = t, m, i = 0;

    function appendText(text){ frag.appendChild(document.createTextNode(text)); }

    while((m = reURL.exec(s))){
      if(m.index > i) appendText(s.slice(i, m.index));
      var a = document.createElement('a'); a.href = m[0]; a.textContent = m[0]; a.target = "_blank"; a.rel = "noopener";
      frag.appendChild(a); i = m.index + m[0].length;
    }
    var rest = s.slice(i);
    var last = 0; var m2;
    while((m2 = reEmail.exec(rest))){
      if(m2.index > last) appendText(rest.slice(last, m2.index));
      var a2 = document.createElement('a'); a2.href = "mailto:"+m2[0]; a2.textContent = m2[0];
      frag.appendChild(a2); last = m2.index + m2[0].length;
    }
    if(last < rest.length) appendText(rest.slice(last));

    if(frag.childNodes.length){
      node.parentNode.replaceChild(frag, node);
    }
  }
  function enhanceKeyLines(container){
    container.querySelectorAll('p').forEach(function(p){
      if(p.classList.contains('keyline')) return;
      var txt = (p.textContent || "").trim();
      var m = txt.match(/^\s*([A-Za-zȘșȚțĂăÂâÎî0-9 _\-/]+?)\s*:\s*(.+)$/);
      if(!m) return;
      p.classList.add('keyline');
      p.textContent = "";
      var strong = document.createElement('strong');
      strong.textContent = m[1] + ":";
      p.appendChild(strong);
      p.appendChild(document.createTextNode(" " + m[2]));
    });
  }
  function renderCheckboxes(ul){
    ul.querySelectorAll('li').forEach(function(li){
      var t = (li.textContent || "").trim();
      var checked = /^\[(?:x|X)\]\s+/.test(t);
      var unchecked = /^\[\s?\]\s+/.test(t);
      if(checked || unchecked){
        li.textContent = t.replace(/^\[(?:x|X)?\]\s+/, '');
        var box = document.createElement('span');
        box.className = "chk" + (checked ? " on" : "");
        li.insertBefore(box, li.firstChild);
      }
    });
  }
  function buildList(lines){
    var reNumList = /^\s*(\d+)[\.\)]\s+/;
    var reDashBullet = /^\s*[–-]\s+/;
    var reDotBullet = /^\s*[•]\s+/;
    var ordered = lines.every(function(l){ return reNumList.test(l); });
    var ulBullets = lines.filter(function(l){ return reDashBullet.test(l) || reDotBullet.test(l); }).length;
    if(ordered){
      var ol = document.createElement('ol');
      lines.forEach(function(l){
        var text = l.replace(reNumList, '');
        var li = document.createElement('li');
        li.textContent = text.trim();
        ol.appendChild(li);
      });
      return ol;
    } else if(ulBullets > 0){
      var ul = document.createElement('ul');
      lines.forEach(function(l){
        var text = l.replace(reDashBullet, '').replace(reDotBullet, '');
        var li = document.createElement('li');
        li.textContent = text.trim();
        ul.appendChild(li);
      });
      renderCheckboxes(ul);
      return ul;
    } else {
      var div = document.createElement('div');
      lines.forEach(function(l){
        var p = document.createElement('p');
        p.textContent = l.trim();
        div.appendChild(p);
      });
      return div;
    }
  }
  function wrapAndToggle(td){
    if(td.querySelector('.desc-box')) return;
    var box = document.createElement('div'); box.className = 'desc-box';
    while(td.firstChild){ box.appendChild(td.firstChild); }
    td.appendChild(box);
    box.classList.add('collapsed');
    requestAnimationFrame(function(){
      var needs = box.scrollHeight > box.clientHeight + 4;
      if(!needs){ box.classList.remove('collapsed'); return; }
      var btn = document.createElement('button');
      btn.className = 'desc-toggle'; btn.type = 'button';
      btn.setAttribute('aria-expanded','false'); btn.textContent = 'Show more';
      btn.addEventListener('click', function(){
        var ex = btn.getAttribute('aria-expanded') === 'true';
        btn.setAttribute('aria-expanded', ex ? 'false' : 'true');
        btn.textContent = ex ? 'Show more' : 'Show less';
        box.classList.toggle('collapsed', ex);
      });
      td.appendChild(btn);
    });
  }
  function run(){
    document.querySelectorAll('table').forEach(function(tbl){
      addDescClass(tbl);
    });
    document.querySelectorAll('table tbody td.desc').forEach(function(td){
      // linkify
      var walker = document.createTreeWalker(td, NodeFilter.SHOW_TEXT, null, false);
      var nodes = []; while(walker.nextNode()) nodes.push(walker.currentNode);
      nodes.forEach(linkifyTextNode);
      // normalize simple <br> content -> paras or lists
      normalizeCell(td);
      // key:value emphasis
      enhanceKeyLines(td);
      // wrap + toggle
      wrapAndToggle(td);
    });
  }
  if(document.readyState === 'loading'){ document.addEventListener('DOMContentLoaded', run); } else { run(); }
})();
</script></body>
</html>
